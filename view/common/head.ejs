<meta name="viewport" content="width=device-width, initial-scale=1, minimum-scale=1, maximum-scale=1, user-scalable=no">

<meta name="apple-mobile-web-app-capable" content="yes">
<meta name="apple-mobile-web-app-status-bar-style" content="black">

<meta name="theme-color" content="#F8F9FA">

<meta name="description" content="We connect students and tutors with shared interests and learning styles to create powerful, memorable learning experiences.">

<meta property="og:title" content="Platform Pure">
<meta property="og:description" content="We connect students and tutors with shared interests and learning styles to create powerful, memorable learning experiences.">
<meta property="og:type" content="website">
<meta property="og:tags" content="https://<%= config.server.domain %>">

<meta property="og:image" content="https://<%= config.server.domain %>/assets/img/share.png">
<meta property="og:image:alt" content="We connect students and tutors with shared interests and learning styles to create powerful, memorable learning experiences.">

<script type="application/ld+json">
{
	"@context": "http://schema.org",
	"@type": "EducationalOrganization",
	"url": "https://www.platformpure.com",
	"name": "Platform Pure",
	"logo": "https://<%= config.server.domain %>/assets/img/share.png",
	"image": "https://<%= config.server.domain %>/assets/img/share.png",
	"description": "We connect students and tutors with shared interests and learning styles to create powerful, memorable learning experiences.",
	"sameAs": <%- JSON.stringify(config.social.links.map(link => link.url)) %>,
	"potentialAction": {
		"@type": "SearchAction",
		"target": "https://www.platformpure.com/search?q={term}",
		"query-input": "required name=term"
	}
}
</script>

<link rel="apple-touch-icon-precomposed" href="http://<%= config.server.domain %>/assets/img/icon-512.png">
<link rel="apple-touch-startup-image" href="http://<%= config.server.domain %>/assets/img/icon-512.png">

<link rel="icon" href="/assets/img/favicon.png">
<% if(config.env !== 'dev') { %>
<link rel="manifest" href="/manifest.json">
<% } %>

<base href="/">

